#include <bits/stdc++.h>
using namespace std;

int main() {
long int s,h;
cin>>s>>h;
long int arr[s];
for(long int i=0;i<s;i++)
cin>>arr[i];
long int x=1,point=0,item=0;
while(x!=0)
{
cin>>x;
if(x==1)
{
if(point==0)
continue;
else
point=point-1;
}
else if(x==2)
{
if(point==s-1)
continue;
else
point=point+1;
    
}
else if(x==3)
{
    if(arr[point]==0||item>0)
    continue;
else
{arr[point]--;
item++;
}
}
else if(x==4)
{
if(arr[point]==h || item!=1)
continue;
else
{arr[point]++;
item--;
}
}
else
{ x=0;break;}
}
for(long int i=0;i<s;i++)
 cout<<arr[i]<<" ";
return 0;
}
